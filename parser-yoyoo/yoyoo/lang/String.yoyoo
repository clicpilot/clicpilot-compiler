		int tar = resultRef;
		return tar;
	}
	
	public static string valueOf(long num) {
		return (num+"");
	}
	
	public static string valueOf(int num) {
		return (num+"");
	}
	
	public static string valueOf(float num) {
		return (num+"");
	}
	
