		string tar = resultRef;
		return tar;
	}
	
	public static string upper(string src) {
		ref/*<string>*/ resultRef;
		upper(src, resultRef);
		string tar = resultRef;
		return tar;
	}
	
	public static string lower(string src) {
		ref/*<string>*/ resultRef;
		lower(src, resultRef);
		string tar = resultRef;
		return tar;
	}
	
	public static string quote(string src) {
		ref/*<string>*/ resultRef;
		quote(src, resultRef);
		string tar = resultRef;
		return tar;
	}
	
	public static int length(string src) {
		ref/*<int>*/ resultRef;
		length(src, resultRef);
		int tar = resultRef;
