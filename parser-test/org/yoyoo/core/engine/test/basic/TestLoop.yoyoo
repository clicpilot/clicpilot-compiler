package org.yoyoo.core.engine.test.basic;

public testCase TestLoop
{
	
	public void test(){
	
		// test for
		int j = 0;
		for(int i=0;i<10;i++) {
			j = i;
		}
		assertEquals(j, 9);
		
		//test while
		j = 0;
		int i = 0;
		while(i<10) {
			j = i;
			i++;
		}
		assertEquals(j, 9);
		
		//test do while
		j = 0;
		i = 0;
		do {
			i++;
			j = i;
		}
		while(i<10);		
		assertEquals(j, 10);
		int k= -1;
		assertEquals(j++, 10);
		assertEquals(j, 11);

		
	}
	

}
